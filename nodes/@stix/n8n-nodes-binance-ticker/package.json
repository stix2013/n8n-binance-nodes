        ],
        "nodes": [
            "dist/nodes/BinanceTicker/BinanceTicker.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "^0.21.0",
        "@types/bun": "^1.3.9",
        "@types/node": "^22.19.11",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.2.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "^2.10.0"
    }
